{
  "name": "AC Column Estimate Counter",
  "version": "0.1",
  "description": "Count some columns.",
  "permissions": ["declarativeContent", "tabs"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "https://app.activecollab.com/*"
      ],
      "js": ["content.js"]
    }
  ],
  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  },
  "manifest_version": 2
}
